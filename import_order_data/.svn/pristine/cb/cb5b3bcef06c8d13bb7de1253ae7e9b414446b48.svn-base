/**
	登录页背景色渐变
**/   
.LoginBody{   
    FILTER: progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#71B9E3,endColorStr=#fafafa); /*IE*/   
    background:-moz-linear-gradient(top,#71B9E3,#fafafa);/*火狐*/   
    background:-webkit-gradient(linear, 0% 0%, 0% 100%,from(#71B9E3), to(#fafafa));/*谷歌*/   
  
    background-image: -webkit-gradient(linear,left bottom,left top,color-start(0, #71B9E3),color-stop(1, #fafafa));/* Safari & Chrome*/  
    filter:  progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#71B9E3', endColorstr='#fafafa'); /*IE6 & IE7*/  
    -ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#71B9E3', endColorstr='#fafafa')"; /* IE8 */  
}
/**
   North页背景色渐变
**/   
.NorthBody{   
FILTER: progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#BED3F0,endColorStr=#fafafa); /*IE*/   
background:-moz-linear-gradient(top,#BED3F0,#fafafa);/*火狐*/   
background:-webkit-gradient(linear, 0% 0%, 0% 160%,from(#BED3F0), to(#fafafa));/*谷歌*/   

background-image: -webkit-gradient(linear,left bottom,left top,color-start(0, #BED3F0),color-stop(1, #fafafa));/* Safari & Chrome*/  
filter:  progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#BED3F0', endColorstr='#fafafa'); /*IE6 & IE7*/  
-ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#BED3F0', endColorstr='#fafafa')"; /* IE8 */  

