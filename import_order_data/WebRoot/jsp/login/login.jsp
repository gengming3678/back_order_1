<script type="text/javascript">
  function mycheck(){ 
	    if(form1.name.value==""){
	   	 alert("账号不能为空！");
		 form1.name.focus();
		 return false;
	    }else if(form1.password.value==""){ 
		    alert("密码不能为空！");
		 	form1.password.focus();
		 return false;
	 	}else{
	 		form1.submit();
	 	}
 }
</script>
